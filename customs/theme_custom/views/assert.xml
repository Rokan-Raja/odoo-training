<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="theme_custom_assets" inherit_id="website.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/header.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/footer.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/about_banner.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/about_background.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/newsletter.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/hr_line.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_custom/static/src/css/heading_center.css"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/theme_custom/static/src/js/header.js"/>
                <script type="text/javascript" src="/theme_custom/static/src/js/send_mail.js"/>
            </xpath>
        </template>
    </data>
</odoo>