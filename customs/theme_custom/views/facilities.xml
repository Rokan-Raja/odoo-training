<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="facilities_page" model="website.page">
        <field name="name">Facilities</field>
        <field name="website_published">True</field>
        <field name="url">/facilities</field>
        <field name="type">qweb</field>
        <field name="key">theme_custom.facilities_page</field>
        <field name="arch" type="xml">
            <t t-name="theme_tutorial.services_page_template">
                <t t-call="website.layout">
                    <div id="wrap">
                        <div class="oe_structure"/>
                    </div>
                </t>
            </t>
        </field>
    </record>
    <record id="facilities_page_link" model="website.menu">
        <field name="name">Facilities</field>
        <field name="page_id" ref="facilities_page"/>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">13</field>
    </record>
</odoo>