<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="place_into_bar" inherit_id="website.snippets" name="Place into bar">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="theme_custom.about_banner"/>
            <t t-snippet="theme_custom.about_background"/>
            <t t-snippet="theme_custom.news_letter"/>
            <t t-snippet="theme_custom.hr_line"/>
            <t t-snippet="theme_custom.heading_center"/>
        </xpath>
    </template>
</odoo>